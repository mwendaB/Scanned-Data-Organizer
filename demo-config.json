{
  "name": "Scanned Data Organizer Demo",
  "version": "1.0.0",
  "description": "A complete demonstration of the OCR document processing application",
  "demo": {
    "features": [
      "User Authentication with Supabase",
      "File Upload with Drag & Drop",
      "OCR Text Extraction with Tesseract.js", 
      "Smart Data Parsing and Structuring",
      "Interactive Data Tables with Filtering",
      "Export to Multiple Formats (CSV, Excel, PDF)",
      "Responsive Design with Tailwind CSS"
    ],
    "testFiles": {
      "simpleText": "Create an image with text like 'Name: John Doe, Age: 30, City: New York'",
      "simpleTable": "Screenshot a basic table with columns and rows",
      "invoice": "Upload a scanned invoice or receipt image"
    },
    "quickStart": {
      "1": "Run 'npm install' to install dependencies",
      "2": "Copy .env.example to .env.local and add Supabase credentials", 
      "3": "Run database migration in Supabase SQL editor",
      "4": "Start with 'npm run dev'",
      "5": "Visit http://localhost:3000 to begin"
    }
  },
  "technologies": {
    "frontend": {
      "framework": "Next.js 14",
      "language": "TypeScript",
      "styling": "Tailwind CSS",
      "stateManagement": "Zustand",
      "components": "Custom UI Components"
    },
    "backend": {
      "database": "Supabase PostgreSQL",
      "authentication": "Supabase Auth",
      "storage": "Supabase Storage",
      "realtime": "Supabase Real-time"
    },
    "processing": {
      "ocr": "Tesseract.js",
      "dataTable": "TanStack Table",
      "fileUpload": "React Dropzone",
      "export": "XLSX.js & FileSaver"
    }
  },
  "fileStructure": {
    "essential": [
      "src/app/page.tsx - Main application entry",
      "src/components/Dashboard.tsx - Core dashboard component", 
      "src/components/FileUpload.tsx - File upload interface",
      "src/components/DataTable.tsx - Interactive data table",
      "src/lib/ocr.ts - OCR processing service",
      "src/lib/supabase.ts - Database client",
      "supabase/migrations/001_initial_schema.sql - Database schema"
    ]
  },
  "readyToRun": true,
  "documentation": [
    "README.md - Project overview and quick start",
    "QUICKSTART.md - 5-minute setup guide", 
    "SETUP.md - Detailed setup instructions",
    "FEATURES.md - Complete feature documentation"
  ]
}
